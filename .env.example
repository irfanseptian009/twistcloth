# AI Fashion Assistant Environment Configuration

# Gemini AI API Key (Required for AI Stylist Chatbot)
# Get your API key from: https://aistudio.google.com/app/apikey
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Firebase Configuration (Existing)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_auth_domain
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_storage_bucket
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Cloudinary Configuration (Existing)
VITE_CLOUDINARY_CLOUD_NAME=your_cloud_name
VITE_CLOUDINARY_API_KEY=your_api_key
VITE_CLOUDINARY_API_SECRET=your_api_secret

# Additional AI Features Configuration (Optional)
# Enable/disable features for production
VITE_ENABLE_SKIN_DETECTION=true
VITE_ENABLE_AI_CHATBOT=true
VITE_ENABLE_WEBCAM_CAPTURE=true

# Performance Settings
VITE_MAX_IMAGE_SIZE=5242880  # 5MB in bytes
VITE_API_RATE_LIMIT=10       # requests per minute
VITE_CHAT_MESSAGE_LIMIT=100  # max messages in chat history
